{
  "name": "camapilates-com",
  "version": "0.0.0",
  "exports": "./src/main.tsx",
  "tasks": {
    "dev": "deno run --allow-all npm:vite",
    "prebuild": "deno run --allow-read scripts/validate-content.ts && deno run --allow-all scripts/generate-og.cjs && deno run --allow-all scripts/generate-sitemap.cjs",
    "build": "deno run --allow-all npm:vite build && deno run --allow-all scripts/static-prerender.cjs",
    "build:dev": "deno run --allow-all npm:vite build --mode development",
    "lint": "deno lint",
    "preview": "deno run --allow-all npm:vite preview"
  },
  "imports": {
    "@/": "./src/",
    "react": "npm:react@^18.3.1",
    "react-dom": "npm:react-dom@^18.3.1",
    "react-dom/client": "npm:react-dom@^18.3.1/client",
    "react-router-dom": "npm:react-router-dom@^6.26.2",
    "lucide-react": "npm:lucide-react@^0.462.0",
    "tailwind-merge": "npm:tailwind-merge@^2.5.2",
    "class-variance-authority": "npm:class-variance-authority@^0.7.1",
    "clsx": "npm:clsx@^2.1.1",
    "@calcom/embed-react": "npm:@calcom/embed-react@^1.5.3",
    "@hookform/resolvers": "npm:@hookform/resolvers@^3.9.0",
    "@radix-ui/react-accordion": "npm:@radix-ui/react-accordion@^1.2.0",
    "@radix-ui/react-alert-dialog": "npm:@radix-ui/react-alert-dialog@^1.1.1",
    "@radix-ui/react-aspect-ratio": "npm:@radix-ui/react-aspect-ratio@^1.1.0",
    "@radix-ui/react-avatar": "npm:@radix-ui/react-avatar@^1.1.0",
    "@radix-ui/react-checkbox": "npm:@radix-ui/react-checkbox@^1.1.1",
    "@radix-ui/react-collapsible": "npm:@radix-ui/react-collapsible@^1.1.0",
    "@radix-ui/react-context-menu": "npm:@radix-ui/react-context-menu@^2.2.1",
    "@radix-ui/react-dialog": "npm:@radix-ui/react-dialog@^1.1.2",
    "@radix-ui/react-dropdown-menu": "npm:@radix-ui/react-dropdown-menu@^2.1.1",
    "@radix-ui/react-hover-card": "npm:@radix-ui/react-hover-card@^1.1.1",
    "@radix-ui/react-label": "npm:@radix-ui/react-label@^2.1.0",
    "@radix-ui/react-menubar": "npm:@radix-ui/react-menubar@^1.1.1",
    "@radix-ui/react-navigation-menu": "npm:@radix-ui/react-navigation-menu@^1.2.0",
    "@radix-ui/react-popover": "npm:@radix-ui/react-popover@^1.1.1",
    "@radix-ui/react-progress": "npm:@radix-ui/react-progress@^1.1.0",
    "@radix-ui/react-radio-group": "npm:@radix-ui/react-radio-group@^1.2.0",
    "@radix-ui/react-scroll-area": "npm:@radix-ui/react-scroll-area@^1.1.0",
    "@radix-ui/react-select": "npm:@radix-ui/react-select@^2.1.1",
    "@radix-ui/react-separator": "npm:@radix-ui/react-separator@^1.1.0",
    "@radix-ui/react-slider": "npm:@radix-ui/react-slider@^1.2.0",
    "@radix-ui/react-slot": "npm:@radix-ui/react-slot@^1.1.0",
    "@radix-ui/react-switch": "npm:@radix-ui/react-switch@^1.1.0",
    "@radix-ui/react-tabs": "npm:@radix-ui/react-tabs@^1.1.0",
    "@radix-ui/react-toast": "npm:@radix-ui/react-toast@^1.2.1",
    "@radix-ui/react-toggle": "npm:@radix-ui/react-toggle@^1.1.0",
    "@radix-ui/react-toggle-group": "npm:@radix-ui/react-toggle-group@^1.1.0",
    "@radix-ui/react-tooltip": "npm:@radix-ui/react-tooltip@^1.1.4",
    "@resvg/resvg-js": "npm:@resvg/resvg-js@^2.6.2",
    "@tanstack/react-query": "npm:@tanstack/react-query@^5.56.2",
    "cmdk": "npm:cmdk@^1.0.0",
    "date-fns": "npm:date-fns@^3.6.0",
    "embla-carousel-react": "npm:embla-carousel-react@^8.3.0",
    "gray-matter": "npm:gray-matter@^4.0.3",
    "input-otp": "npm:input-otp@^1.2.4",
    "next-themes": "npm:next-themes@^0.3.0",
    "react-day-picker": "npm:react-day-picker@^8.10.1",
    "react-helmet-async": "npm:react-helmet-async@^2.0.5",
    "react-hook-form": "npm:react-hook-form@^7.53.0",
    "react-markdown": "npm:react-markdown@^10.1.0",
    "react-resizable-panels": "npm:react-resizable-panels@^2.1.3",
    "recharts": "npm:recharts@^2.12.7",
    "rehype-autolink-headings": "npm:rehype-autolink-headings@^7.1.0",
    "rehype-highlight": "npm:rehype-highlight@^7.0.2",
    "rehype-raw": "npm:rehype-raw@^7.0.0",
    "remark-gfm": "npm:remark-gfm@^4.0.1",
    "remark-slug": "npm:remark-slug@^7.0.1",
    "satori": "npm:satori@^0.18.3",
    "sonner": "npm:sonner@^1.5.0",
    "tailwindcss-animate": "npm:tailwindcss-animate@^1.0.7",
    "vaul": "npm:vaul@^0.9.3",
    "zod": "npm:zod@^3.23.8",
    "vite": "npm:vite@^5.4.1",
    "@vitejs/plugin-react-swc": "npm:@vitejs/plugin-react-swc@^3.5.0",
    "buffer": "npm:buffer@^6.0.3"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "lib": ["DOM", "DOM.Iterable", "ES6", "ES2017", "ES2018", "ES2019", "ES2020", "ES2021", "ES2022"],
    "allowJs": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleDetection": "force"
  },
  "lint": {
    "rules": {
      "tags": ["recommended"]
    }
  },
  "fmt": {
    "useTabs": false,
    "lineWidth": 80,
    "indentWidth": 2,
    "semiColons": true,
    "singleQuote": false,
    "proseWrap": "preserve",
    "include": ["src/", "scripts/"],
    "exclude": ["dist/", "node_modules/", ".git/"]
  }
}
